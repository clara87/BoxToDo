@model IEnumerable<DataAccessLayer.Carpeta>


<ul class="list-group">
    @foreach (var item in Model)
    {
        
    <li class="list-group-item d-flex justify-content-between align-items-center">
        <a href="/Carpetas/Tareas/@item.IdCarpeta">@Html.DisplayFor(modelItem => item.Nombre)</a>
    </li>


    }
</ul>